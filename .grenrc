{
    "dataSource": "prs",
    "ignoreIssuesWith": [
        "duplicate",
        "dependencies",
        "documentation",
        "help wanted",
        "invalid",
        "question",
        "wontfix"
    ],
    "template": {
        "issue": "- {{name}} | [{{text}}]({{url}}) <sub>by [{{user_login}}]({{user_url}})</sub>",
        "group": "\n### {{heading}}\n",
        "changelogTitle": "",
        "release": "## Changes since last release\n{{body}}"
    },
    "groupBy": {
        "✨ New Features": "[enhancement]",
        "🛠  Improvements": "[tweak]",
        "🐛 Bug Fixes:": "[bug]"
    }
}